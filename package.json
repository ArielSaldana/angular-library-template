{
  "name": "angular-angular-module-library",
  "version": "1.0.0",
  "description": "Starting point for developing Angular modules and publishing them on NPM",
  "keywords": [
    "angular 2"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .tmp && rimraf dist",
    "transpile": "ngc",
    "copy": "cpx './package-dist.json' dist && renamer --find 'package-dist.json' --replace 'package.json' ./dist/*",
    "build": "npm run clean && npm run transpile && node node_modules/.bin/webpack && npm run copy"
  },
  "license": "MIT",
  "bugs": {},
  "repository": {
    "type": "git"
  },
  "devDependencies": {
    "@angular/compiler": "^2.4.4",
    "@angular/compiler-cli": "^2.4.4",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "clean-webpack-plugin": "^0.1.16",
    "cpx": "^1.5.0",
    "renamer": "^0.6.1",
    "rimraf": "^2.5.4",
    "typescript": "~2.0.0",
    "uglify-js": "^2.7.5",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/core": "^2.4.4",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.6"
  }
}
